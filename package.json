{
  "name": "trello-api",
  "version": "1.0.0",
  "description": "Sowndev",
  "main": "src/server.ts",
  "scripts": {
    "lint": "eslint src --ext js,ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist",
    "build-babel": "babel ./src -d ./dist",
    "build": "npm run clean && tsc && tsc-alias",
    "production": "npm run build && cross-env BUILD_MODE=production node ./dist/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec ts-node ./src/server.ts "
  },
  "author": "Sowndev",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/async-exit-hook": "^2.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/lodash": "^4.14.202",
    "@types/mongodb": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.2.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "async-exit-hook": "^2.0.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongodb": "^6.3.0",
    "nodemon": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8"
  },
  "module-alias": {
    "src": "./src"
  }
